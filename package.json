{
  "name": "vite-plugin-stylex-next",
  "version": "0.1.0",
  "description": "A vite plugin for stylex by FaceBook (Meta)",
  "scripts": {
    "dev": "unbuild --stub",
    "build": "unbuild",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vite-plugin-stylex-next/vite-plugin-stylex-next.git"
  },
  "keywords": [
    "vite",
    "stylex"
  ],
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "author": "Michael Chong",
  "license": "MIT",
  "engines": {
    "node": "^14.18.0 || >=16.0.0"
  },
  "peerDependencies": {
    "vite": "^4.2.0 || ^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.5",
    "@babel/plugin-syntax-flow": "^7.23.3",
    "@babel/plugin-syntax-jsx": "^7.23.3",
    "@babel/plugin-syntax-typescript": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@types/babel__core": "^7.20.5",
    "@types/node": "^20.10.4",
    "unbuild": "^2.0.0"
  },
  "dependencies": {
    "@stylexjs/babel-plugin": "^0.3.0"
  }
}